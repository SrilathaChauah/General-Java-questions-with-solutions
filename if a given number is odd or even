public class Welcome {
   public static void main(String args[]){
       int a = 11;
       if (a==0)
       {
           System.out.println("Number is Even");
       }
       if (a%2 ==0)
       {
           System.out.println("Number is Even");
       }
       else
       {
           System.out.println("Number is Odd");
       }
   }
}
